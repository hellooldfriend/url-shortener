  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.9.21",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "shortid": "^2.2.15"
  }
}
